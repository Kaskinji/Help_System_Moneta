:квалифицированная_электронная_подпись rdf:type comcore:Resource ;
    rdfs:label "Квалифицированная электронная подпись"@ru ;
    comcore:mentionedIn :7 ;
    dc:definition "Квалифицированная электронная подпись (КЭП) – электронная подпись, созданная с использованием ключа электронной подписи, соответствующего квалифицированному сертификату ключа проверки электронной подписи, выданного аккредитованным удостоверяющим центром и полностью соответствующая требованиям федерального законодательства."@ru ;
    comcore:relation :ключ_электронной_подписи .

:ключ_электронной_подписи rdf:type comcore:Resource ;
    rdfs:label "ключ электронной подписи"@ru ;
